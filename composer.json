{
    "name": "notadd/member",
    "description": "Notadd's Member Module.",
    "keywords": ["notadd", "cms", "framework", "member"],
    "homepage": "https://notadd.com",
    "license": "Apache-2.0",
    "type": "notadd-module",
    "authors": [
        {
            "name": "Notadd",
            "email": "notadd@ibenchu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Notadd\\Member\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notadd\\Member\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "notadd/installers": "0.14.*",
        "notadd/testing": "0.4.*",
